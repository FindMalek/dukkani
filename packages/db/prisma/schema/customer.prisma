model Customer {
  id        String   @id @default(cuid())
  name      String
  phone     String
  storeId   BigInt   @map("storeid")
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  store  Store   @relation(fields: [storeId], references: [id], onDelete: Cascade)
  orders Order[]

  @@unique([phone, storeId])
  @@map("customer")
}
