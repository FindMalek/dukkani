# Database
DATABASE_URL=postgresql://user:password@localhost:5432/dukkani

# Telegram
TELEGRAM_BOT_NAME=your_bot_name

# OpenTelemetry 
OTEL_SERVICE_NAME=dukkani-storefront
OTEL_SAMPLING_RATE=1.0
OTEL_ENABLED=true

# Grafana Cloud OTLP Endpoint (optional - only if using Grafana Cloud)
OTEL_EXPORTER_OTLP_ENDPOINT=
OTEL_EXPORTER_OTLP_HEADERS=Authorization=Basic%20YOUR_BASE64_ENCODED_CREDENTIALS
OTEL_EXPORTER_OTLP_COMPRESSION=gzip
OTEL_EXPORTER_OTLP_PROTOCOL=http/protobuf

# Client URLs
NEXT_PUBLIC_NODE_ENV=local
NEXT_PUBLIC_CORS_ORIGIN=http://localhost:3004
NEXT_PUBLIC_ALLOWED_ORIGIN=*
NEXT_PUBLIC_STORE_DOMAIN=localhost:3004